{
    "name": "hc-zenburn",
    "type": "dark",
    "include": "./defaults.json",
    "tokenColors": [
        {
            "settings": {
                "foreground": "#DCDCCC"
            }
        },
        {
            "scope": [
                "meta.embedded",
                "source.groovy.embedded"
            ],
            "settings": {
                "foreground": "#DCDCCC"
            }
        },
        {
            "name": "Parameters",
            "scope": "variable.parameter",
            "settings": {
                "foreground": "#ECBC9C"
            }
        },
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#8CAC8C"
            }
        },
        {
            "name": "Documentations",
            "scope": [
                "comment.line.documentation",
                "comment.block.documentation",
                "string.quoted.docstring.multi",
            ],
            "settings": {
                "foreground": "#ACD2AC"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation.definition.string",
                "punctuation.definition.variable",
                "punctuation.definition.string",
                "punctuation.definition.parameters",
                "punctuation.definition.string",
                "punctuation.definition.array"
            ],
            "settings": {
                "foreground": "#d3af86" // TODO
            }
        },
        {
            "name": "Delimiters",
            "scope": "none",
            "settings": {
                "foreground": "#ff0000" // TODO
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator",
                "punctuation.separator",
                "storage.modifier.pointer", // cpp *
                "storage.modifier.reference", // cpp &
                "source.ocaml support.type", // ocaml ->, |
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#EDDCAC"
            }
        },
        {
            "name": "Preproccesors",
            "scope": [
                "keyword.control.directive", // cpp #include, ifndef, ...
                "meta.preprocessor",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#9CC7FB"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.operator.new.cpp",
                "keyword.operator.delete.cpp",
                "keyword.other.using",
                "keyword.other.operator",
                "storage.type",
                "storage.modifier.const", // cpp const
                "storage.modifier.static", // cpp static
                "storage.modifier.inline", // cpp inline
                "storage.modifier.specifier.const", // cpp const
                "punctuation.definition.tag", // ocaml struct, end
                "source.ocaml entity.name.filename" // ocaml module
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#FDECBC"
            }
        },
        {
            "name": "This/Self",
            "scope": [
                "variable.language"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#DDCC9C"
            }
        },
        {
            "name": "Member-accesses",
            "scope": "meta.member.access",
            "settings": {
                "foreground": "#C4AD9E"
            }
        },
        {
            "name": "Namespace-accesses",
            "scope": [
                "entity.name.scope-resolution", // namespace access
            ],
            "settings": {
                "foreground": "#CCF8CC"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "meta.function-call.arguments",
            ],
            "settings": {
                "foreground": "#DCDCCC"
            }
        },
        {
            "name": "enumMember",
            "scope": "variable.other.enummember",
            "settings": {
                "foreground": "#89C5C8"
            }
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "meta.require",
                "support.function.any-method",
                "meta.function-call.generic",
            ],
            "settings": {
                "foreground": "#A0EDF0"
            }
        },
        {
            "name": "Types",
            "scope": [
                "support.class",
                "entity.name.class",
                "entity.name.type",
                "entity.name.namespace",
                "storage.type.core",
                "support.constant.core",
                "entity.name.type",
                "storage.type.built-in",
                "support.type",
                "storage.type.error",
                "storage.type.numeric",
                "storage.type.string",
                "source.ocaml entity.name.class constant.numeric" // ocaml type
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#89C5C8"
            }
        },
        {
            "name": "Methods",
            "scope": "keyword.other.special-method",
            "settings": {
                "foreground": "#8ab1b0" // TODO
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "foreground": "#89C5C8" // CHECK
            }
        },
        {
            "name": "Support",
            "scope": [
                "support.function",
                "entity.name.function.macro",
                "support.other.macro",
                "entity.name.function.decorator",
            ],
            "settings": {
                "foreground": "#9CC7FB"
            }
        },
        {
            "name": "Strings, Inherited Class",
            "scope": [
                "string",
                "constant.other.symbol",
                "entity.other.inherited-class"
            ],
            "settings": {
                "foreground": "#D9A0A0"
            }
        },
        {
            "name": "Integers",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#CCF8CC"
            }
        },
        {
            "name": "Floats",
            "scope": "none",
            "settings": {
                "foreground": "#CCF8CC"
            }
        },
        {
            "name": "Boolean",
            "scope": "none",
            "settings": {
                "foreground": "#CCF8CC"
            }
        },
        {
            "name": "Constants",
            "scope": "constant",
            "settings": {
                "foreground": "#CCF8CC"
            }
        },
        {
            "name": "Tags",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#dc3958" // TODO
            }
        },
        {
            "name": "Attributes",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#ECBC9C"
            }
        },
        {
            "name": "Attribute IDs",
            "scope": [
                "entity.other.attribute-name.id", // also ocaml type
                "punctuation.definition.entity"
            ],
            "settings": {
                "foreground": "#89C5C8"
            }
        },
        {
            "name": "Selector",
            "scope": "meta.selector",
            "settings": {
                "foreground": "#98676a" // TODO
            }
        },
        {
            "name": "Values",
            "scope": "none",
            "settings": {
                "foreground": "#f79a32" // TODO
            }
        },
        {
            "name": "Headings",
            "scope": [
                "markup.heading",
                "markup.heading.setext",
                "punctuation.definition.heading",
                "entity.name.section"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#8ab1b0"
            }
        },
        {
            "name": "Units",
            "scope": "keyword.other.unit",
            "settings": {
                "foreground": "#DDCC9C"
            }
        },
        {
            "name": "Bold",
            "scope": [
                "markup.bold",
                "punctuation.definition.bold"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#f06431" // TODO
            }
        },
        {
            "name": "Italic",
            "scope": [
                "markup.italic",
                "punctuation.definition.italic"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#98676a" // TODO
            }
        },
        {
            "name": "Code",
            "scope": "markup.inline.raw",
            "settings": {
                "foreground": "#889b4a" // TODO
            }
        },
        {
            "name": "Link Text",
            "scope": "string.other.link",
            "settings": {
                "foreground": "#dc3958" // TODO
            }
        },
        {
            "name": "Link Url",
            "scope": "meta.link",
            "settings": {
                "foreground": "#f79a32" // TODO
            }
        },
        {
            "name": "Lists",
            "scope": "markup.list",
            "settings": {
                "foreground": "#dc3958" // TODO
            }
        },
        {
            "name": "Quotes",
            "scope": "markup.quote",
            "settings": {
                "foreground": "#f79a32" // TODO
            }
        },
        {
            "name": "Separator",
            "scope": "meta.separator",
            "settings": {
                "foreground": "#EDDCAC"
            }
        },
        {
            "name": "Inserted",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#889b4a" // TODO
            }
        },
        {
            "name": "Deleted",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#dc3958" // TODO
            }
        },
        {
            "name": "Changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#98676a" // TODO
            }
        },
        {
            "name": "Colors",
            "scope": "constant.other.color",
            "settings": {
                "foreground": "#7e602c" // TODO
            }
        },
        {
            "name": "Regular Expressions",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#C4AD9E"
            }
        },
        {
            "name": "Escape Characters",
            "scope": "constant.character.escape",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#BEADC9"
            }
        },
        {
            "name": "Embedded",
            "scope": [
                "punctuation.section.embedded",
                "variable.interpolation",
                "constant.character.format.placeholder", // python {}
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#E9B0B0"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid.illegal",
            "settings": {
                "foreground": "#C99090"
            }
        },
        {
            "name": "Attribute",
            "scope": "meta.attribute",
            "settings": {
                "foreground": "#9CC7FB"
            }
        },
        {
            "name": "JSON Key - Level 0",
            "scope": [
                "source.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#EDDCAC"
            }
        },
        {
            "name": "JSON Key - Level 1",
            "scope": [
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#9cd5ec"
            }
        },
        {
            "name": "JSON Key - Level 2",
            "scope": [
                "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#bebaf0"
            }
        },
        {
